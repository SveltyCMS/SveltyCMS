{
	"name": "SimpleCMS",
	"version": "0.0.3",
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite dev --open",
		"build": "vite build",
		"package": "svelte-kit package",
		"prepare": "husky install",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"typesafe-i18n": "typesafe-i18n",
		"start": "npm run dev",
		"stop": "npx kill-port 5173"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "0.124.2",
		"@sveltejs/adapter-node": "latest",
		"@sveltejs/kit": "1.8.3",
		"@types/cookie": "0.5.1",
		"@types/mapbox-gl": "^2.7.10",
		"@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
		"@typescript-eslint/eslint-plugin": "5.53.0",
		"@typescript-eslint/parser": "5.53.0",
		"autoprefixer": "10.4.13",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"eslint": "8.34.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"sharp": "^0.31.3",
		"svelte": "3.55.1",
		"svelte-check": "3.0.3",
		"svelte-preprocess": "5.0.1",
		"tailwindcss": "3.2.7",
		"tslib": "2.5.0",
		"typesafe-i18n": "5.24.1",
		"typescript": "~4.9.5",
		"vite": "4.1.3"
	},
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^3.1.0",
		"@lucia-auth/adapter-mongoose": "0.4.0",
		"@lucia-auth/oauth": "0.5.2",
		"@lucia-auth/sveltekit": "0.6.3",
		"@mapbox/mapbox-gl-geocoder": "^5.0.1",
		"@mapbox/mapbox-gl-language": "^1.0.1",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/line-clamp": "0.4.2",
		"@tailwindcss/typography": "0.5.9",
		"@tanstack/svelte-table": "^8.7.9",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/nodemailer": "6.4.7",
		"axios": "^1.3.3",
		"cheerio": "1.0.0-rc.12",
		"dayjs": "^1.11.7",
		"devalue": "4.3.0",
		"flag-icons": "^6.6.6",
		"get-video-id": "^3.6.5",
		"ioredis": "5.3.1",
		"jsonwebtoken": "^9.0.0",
		"lucia-auth": "0.6.0",
		"mapbox-gl": "^2.12.1",
		"mongoose": "6.9.2",
		"nodemailer": "6.9.1",
		"svelte-lexical": "0.1.8",
		"svelte-tel-input": "^1.1.3",
		"tabulator-tables": "^5.4.4",
		"tiktok-scraper": "^1.4.36",
		"zod": "^3.20.6"
	}
}
