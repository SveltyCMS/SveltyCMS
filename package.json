{
	"name": "sveltycms",
	"description": "A powerful Headless CMS with sveltekit power",
	"version": "0.0.4",
	"scripts": {
		"---- DEVELOP ----------------------------------------------------------": "",
		"dev": "vite dev",
		"devv": "LAUNCH_EDITOR='code' vite --open",
		"inlang": "paraglide-js compile --project ./project.inlang",
		"---- BUILD ----------------------------------------------------------": "",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"---- CHECK & TEST ----------------------------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && playwright test",
		"test:e2e": "pnpm dev & pnpx wait-on http://localhost:5173 && playwright test",
		"---- LINT & FORMAT ------------------------------------------------------------": "",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"---- OTHER ----------------------------------------------------------": "",
		"preview": "vite preview"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rar9/SveltyCMS"
	},
	"bugs": {
		"url": "https://github.com/Rar9/SveltyCMS/issues"
	},
	"keywords": [
		"cms",
		"content-management-system",
		"DXP Digital",
		"Developer Experience Platform",
		"inlang",
		"i18n",
		"internationalization",
		"paraglide-js",
		"headless",
		"headless-cms",
		"graphql",
		"skeleton",
		"svelte",
		"svelte-cms",
		"sveltekit",
		"sveltekit_cms",
		"sveltycms",
		"oauth",
		"tailwind",
		"tailwindcss",
		"rich-text-editor",
		"lexical-editor",
		"mongodb"
	],
