{
	"name": "sveltycms",
	"description": "A powerful Headless CMS with sveltekit power",
	"version": "0.0.5",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"-- INSTALLER -----------------------------------------": "",
		"installer": "node cli-installer/cli-installer.js",
		"-- DEVELOP -------------------------------------------": "",
		"dev": "vite dev",
		"devv": "LAUNCH_EDITOR='code' vite --open",
		"-- BUILD ---------------------------------------------": "",
		"build": "bun run vite build",
		"-- CHECK & TEST ---------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
		"check:watch": " svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && bun test",
		"test:bun": "bun test tests/bun",
		"test:playwright": "bun x playwright test tests/playwright",
		"test:all": "bun run test && bun run test:playwright",
		"-- LINT & FORMAT ---------------------------------------": "",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"-- OTHER ----------------------------------------------": "",
		"preview": "bun run vite preview",
		"paraglide": "paraglide-js compile --project ./project.inlang",
		"translate": "bun x @inlang/cli machine translate --project ./project.inlang",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"license": "MIT",
	"homepage": "https://SveltyCMS.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/SveltyCMS/SveltyCMS"
	},
	"bugs": {
		"url": "https://github.com/SveltyCMS/SveltyCMS/issues"
	},
