# Use SQLite or disable database for testing
DB_TYPE=sqlite
DB_URL=sqlite:./test.db
NODE_ENV=test
PUBLIC_ENVIRONMENT=development

# Disable features that require MongoDB
DISABLE_DB_INIT=true