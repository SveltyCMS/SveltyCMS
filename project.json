{
    "name": "sveltycms",
    "$schema": "node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "{projectRoot}/build"
            ],
            "options": {
                "command": "vite build",
                "cwd": "{projectRoot}"
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite dev",
                "cwd": "{projectRoot}"
            }
        },
        "preview": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite preview",
                "cwd": "{projectRoot}"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "outputs": [],
            "options": {
                "command": "eslint . --cache",
                "cwd": "{projectRoot}"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun test",
                "cwd": "{projectRoot}"
            }
        }
    },
    "tags": [
        "type:app",
        "scope:cms"
    ]
}